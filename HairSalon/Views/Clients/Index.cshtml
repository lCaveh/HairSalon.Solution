@{
  Layout = "_Layout";
}
<div class="container">
  <h1>List of clients:</h1>
  @if (@Model.Count == 0)
  {
    <h3>No Clients have been added for this stylist yet!</h3>
  }
  @foreach (var client in Model)
  {
    <h3><a href='/stylists/@stylist.GetId()/clients/@client.GetId()'>@client.GetName() </a> </h3>
    <p>Phone No. : @client.GetPhone()</p>
  }
</div>
